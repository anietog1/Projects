{
 {
  ':', ':=', Identifier, Integer, String, 'PC', '+', '-', '*', '/', '%', 'M', '[', ']',
  'readInt', 'writeInt', 'readStr', 'writeStr', ',', '(', ')', 'goto', 'if', 'then',
  'halt', 'break', 'equ', '>=', '>', '<=', '<', '=', '<>'
 },
 {
  eweprog, executable, labeled_instr, instr, equates, memref, condition
 },
 {
  eweprog ->
   executable equates EOF,
  executable ->
   labeled_instr
   | labeled_instr executable,
  labeled_instr ->
   Identifier rest_labeled_instr
   | instr,
  rest_labeled_instr ->
   ':' labeled_instr
   | ':=' Integer
   | ':=' String
   | ':=' 'PC' + Integer
   | ':=' memref
   | ':=' memref operator memref
   | ':=' 'M' '[' memref '+' Integer ']',
  instr ->
   'M' '[' Integer ']' ':=' Integer
   | 'M' '[' Integer ']' ':=' String
   | 'M' '[' Integer ']' ':=' 'PC' + Integer
   | 'PC' := memref
   | 'M' '[' Integer ']' ':=' memref
   | 'M' '[' Integer ']' ':=' memref operator memref
   | 'M' '[' Integer ']' ':=' 'M' '[' memref '+' Integer ']'
   | 'M' '[' memref + Integer ']' ':=' memref
   | 'readInt' '(' memref ')'
   | 'writeInt' '(' memref ')'
   | 'readStr' '(' memref ',' memref ')'
   | 'writeStr' '(' memref ')'
   | 'goto' Integer
   | 'goto' Identifier
   | 'if' memref condition memref 'then' 'goto' Integer
   | 'if' memref condition memref 'then' 'goto' Identifier
   | 'halt'
   | 'break',
  equates ->
   null
   | 'equ' Identifier 'M' '[' Integer ']' equates,
  memref ->
   'M' '[' Integer ']'
   | Identifier,
  operator ->
   '+'
   | '-'
   | '*'
   | '/'
   | '%',
  condition ->
   '>='
   | '>'
   | '<='
   | '<'
   | '='
 },
 eweprog
}